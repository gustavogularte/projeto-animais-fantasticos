{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-console": 0,
        "import/extensions":0,
        "class-methods-use-this":0,
        "max-classes-per-file": 0
    }
}
